{
  "canisters": {
    "registry": {
      "main": "canisters/filetree-registry.mo",
      "type": "motoko"
    },
    "storage": {
      "main": "canisters/file-storage.mo",
      "type": "motoko"
    },
    "test": {
      "main": "canisters/test.mo",
      "type": "motoko"
    },
    "assets": {
      "dependencies": [
        "registry",
        "storage",
        "test"
      ],
      "frontend": {
        "entrypoint": "dist/index.html"
      },
      "source": [
        "dist/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}
